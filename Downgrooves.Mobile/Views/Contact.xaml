             x:Class="Downgrooves.Mobile.Views.Contact"
             xmlns:viewModels="clr-namespace:Downgrooves.Mobile.ViewModels"
             NavigationPage.HasNavigationBar="false"
             Title="Contact Us" x:DataType="viewModels:ContactViewModel">
